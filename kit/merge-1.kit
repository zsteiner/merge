<!DOCTYPE html>
<html>
	<head>
		<title>Merge.</title>
		<meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- @include 'head' -->
	</head>
	<body>
        <!-- @include 'svg-icons' -->
		<div class="page-wrap">
            <!-- @include 'header' -->

            <main role="main"> 
                <!--<p class="note"></p> -->

                <nav class="app-nav">   
                    <div>
                        <span class="app-nav-first"><a href="merge-overview-1.html" class="button b-alert"><svg class="icon"><use xlink:href="#icon-cancel"></use></svg>cancel merge</a></span>
                        <span class="app-nav-prev is-hidden"><a class="button app-prev"><svg class="icon"><use xlink:href="#icon-arrow_back"></use></svg>previous conflict</a></span>
                    </div>
                    <div>
                        <span class="app-nav-prev"><a class="button icon-right app-next">next conflict <svg class="icon"><use xlink:href="#icon-arrow_forward"></use></svg></a></span>
                        <span class="app-nav-penultimate is-hidden"><a class="button icon-right app-next">finish merge <svg class="icon"><use xlink:href="#icon-arrow_forward"></use></svg></a></span>
                        <span class="app-nav-last is-hidden"><a href="merge-final-1.html" class="button b-success"><svg class="icon"><use xlink:href="#icon-check"></use></svg>approve merge</a></span>
                    </div>
                </nav>
                <h2 class="app-heading">Resolve Conflicts</h2>
                <div class="progress-container">    
                    <div class="progress-label">
                        <span class="selected-conflict"></span> of <span class="total-conflicts"></span> conflicts
                    </div>
                    <div class="complete-message is-hidden">
                        Conflicts Resolved
                    </div>
                    <div class="progress">
                        <div class="progress-bar"></div>
                    </div>
                </div>                
                
                <div class="card-container with-keys">
                    <div class="card card-select" data-card-label="A">
                        <h2>
                            <svg class="icon icon-unchecked"><use xlink:href="#icon-unchecked_circle"></use></svg>
                            <svg class="icon icon-checked"><use xlink:href="#icon-check_circle"></use></svg>
                            Employee A
                        </h2>
                        <ol class="card-content">
                            <li class="card-option is-current">
                                <label>Name</label>
                                <span class="value">Bob Sullivan</span>
                            </li>
                            <li class="card-option">
                                <label>Date of Birth</label>
                                <span class="value">Mar 4, 1979</span>
                            </li>
                            <li class="card-option">
                                <label>Location</label>
                                <span class="value">1471, 14th Floor</span>
                            </li>
                            <li class="card-option">
                                <label>Supervisor</label>
                                <span class="value">Molly Hernandez</span>
                            </li>
                            <li class="card-option">
                                <label>Address</label>
                                <div><span class="value">123 Street Rd.</span></div>
                                <div><span class="value">Anytown, SS 99999</span></div>
                            </li>
                        </ol>
                    </div>
                    <div class="card card-select"  data-card-label="B">
                        <h2>
                            <svg class="icon icon-unchecked"><use xlink:href="#icon-unchecked_circle"></use></svg>
                            <svg class="icon icon-checked"><use xlink:href="#icon-check_circle"></use></svg>
                            Employee B
                        </h2>
                        <ol class="card-content">
                            <li class="card-option is-current">
                                <label>Name</label>
                                <span class="value">Robert Sullivan</span>
                            </li>
                            <li class="card-option">
                                <label>Date of Birth</label>
                                <span class="value">Mar 8, 1979</span>
                            </li>
                            <li class="card-option">
                                <label>Location</label>
                                <span class="value">1213, 13th Floor</span>
                            </li>
                            <li class="card-option">
                                <label>Supervisor</label>
                                <span class="value">Thurston Evers</span>
                            </li>
                            <li class="card-option">
                                <label>Address</label>
                                <div><span class="value">123 Street</span></div>
                                <div><span class="value">Anytown, SS 99999</span></div>
                            </li>
                        </ol>
                    </div>
                </div>
                <!-- @include 'merge-information'-->
            </main>    		
		</div>
        <!-- @include 'footer' -->
		<script src="scripts/merge.js" charset="utf-8"></script>
	</body>
</html>
